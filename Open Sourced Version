local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

local CurrentCamera = workspace.CurrentCamera

local Blur = CurrentCamera:FindFirstChild("Blur")
if not Blur then
	Blur = Instance.new("BlurEffect", CurrentCamera)
end
Blur.Size = 0

local isR15 = Character:FindFirstChild("UpperTorso") ~= nil
local R15 = {"Head","UpperTorso","LowerTorso","LeftUpperArm","LeftLowerArm","LeftHand","RightUpperArm","RightLowerArm","RightHand","LeftUpperLeg","LeftLowerLeg","LeftFoot","RightUpperLeg","RightLowerLeg","RightFoot","HumanoidRootPart"}
local R6 = {"Head","Torso","Left Arm","Right Arm","Left Leg","Right Leg","HumanoidRootPart"}

local bodyParts = {}
for _, n in ipairs(isR15 and R15 or R6) do
	local p = Character:FindFirstChild(n)
	if p and p:IsA("BasePart") then
		bodyParts[#bodyParts + 1] = p
	end
end

