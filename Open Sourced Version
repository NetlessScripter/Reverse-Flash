--[[
    REVERSE FLASH SCRIPT - FIXED VERSION
    
    FEATURES:
    - Fixed nil indexing error with _G variables
    - Added comprehensive R15 support (all R15 body parts)
    - Simple table-based user configuration
    
    USAGE:
    -- Define SetupUsers as a TABLE before loading the script:
    _G.SetupUsers = {
        ["YourUsername"] = "RF",
        ["FriendUsername"] = "Zoom",
        ["AnotherFriend"] = "KidFlash"
    }
    
    -- THEN load the script:
    loadstring(game:HttpGet("YOUR_URL_HERE"))()
    
    CONTROLS:
    - Press Q to toggle Speed Mode
    - Press C to toggle Phase Mode
    - Press B to toggle Angular Mirage
    - Press X to Disable your speed
    
    STATE NAMES:
    RF, Zoom, GodOfSpeed, NWA, KidFlash, Regular
--]]

-- Setup user configuration from _G.SetupUsers table
if type(_G.SetupUsers) == "table" then
    local playerName = game:GetService("Players").LocalPlayer.Name
    local speedsterType = _G.SetupUsers[playerName] or "Regular"
    
    -- Reset all flags
    _G.RF = false
    _G.Zoom = false
    _G.GodOfSpeed = false
    _G.NWA = false
    _G.KidFlash = false
    _G.Regular = false
    
    -- Set the appropriate flag
    if speedsterType == "RF" then
        _G.RF = true
        print("✓ Reverse Flash loaded for " .. playerName)
    elseif speedsterType == "Zoom" then
        _G.Zoom = true
        print("✓ Zoom loaded for " .. playerName)
    elseif speedsterType == "GodOfSpeed" then
        _G.GodOfSpeed = true
        print("✓ Godspeed loaded for " .. playerName)
    elseif speedsterType == "NWA" then
        _G.NWA = true
        print("✓ Negative Speed Force loaded for " .. playerName)
    elseif speedsterType == "KidFlash" then
        _G.KidFlash = true
        print("✓ Kid Flash loaded for " .. playerName)
    elseif speedsterType == "Regular" then
        _G.Regular = true
        print("✓ Regular speedster loaded for " .. playerName)
    else
        _G.Regular = true
        print("✓ Regular speedster (default) loaded for " .. playerName)
    end
else
    -- No SetupUsers table found, check for pre-existing global flags
    if not _G.RF and not _G.Zoom and not _G.GodOfSpeed and not _G.NWA and not _G.KidFlash and not _G.Regular then
        _G.Regular = true
        print("✓ Regular speedster (default) loaded for " .. game:GetService("Players").LocalPlayer.Name)
    end
end

Players = game:GetService("Players")
RunService = game:GetService("RunService")
WorkspaceRef = workspace

LocalPlayerRef = Players.LocalPlayer
CharacterRef = LocalPlayerRef.Character or LocalPlayerRef.CharacterAdded:Wait()
HRP = CharacterRef:WaitForChild("HumanoidRootPart")

