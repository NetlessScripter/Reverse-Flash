game:GetService("RunService").RenderStepped:Connect(function()
    local d = LocalPlayerRef.Character.Humanoid.MoveDirection
    if d.Magnitude > 0 then HRP.CFrame = CFrame.new(HRP.Position, HRP.Position + d) end
end)

local snap=18

RunService.RenderStepped:Connect(function(dt)
	if h.FloorMaterial==Enum.Material.Air then return end

	local md=h.MoveDirection
	if md.Magnitude==0 then return end

	md=Vector3.new(md.X,0,md.Z).Unit

	local v=HRP.AssemblyLinearVelocity
	local y=v.Y

	local sp=v.Magnitude
	if sp<1 then return end

	local target=md*sp
	HRP.AssemblyLinearVelocity=v:Lerp(Vector3.new(target.X,y,target.Z),math.clamp(dt*snap,0,1))
end)

_G.AddUser = AddAllowedUser
_G.RemoveUser = RemoveAllowedUser
_G.SetUserState = SetUserState
_G.ClearUsers = function()
    AllowedUsers = {}
    for player, data in pairs(PlayerVisualData) do
        if player ~= LocalPlayerRef then
            CleanupPlayerVisuals(player)
        end
    end
    print("All allowed users cleared!")
end

_G.GetAllowedUsers = function()
    print("=== Allowed Users ===")
    local count = 0
    for username, state in pairs(AllowedUsers) do
        count = count + 1
        print(count .. ". " .. username .. " - State: " .. state)
    end
    if count == 0 then
        print("No users added yet!")
    end
    print("====================")
    return AllowedUsers
end

_G.RefreshVisuals = function()
    for _, player in pairs(Players:GetPlayers()) do
        if CanSeeVisuals(player) and player.Character then
            CleanupPlayerVisuals(player)
            task.wait(0.1)
            InitializePlayerVisuals(player)
        end
    end
    print("Visuals refreshed for all allowed users!")
end

_G.AddCurrentPlayers = function(defaultState)
    defaultState = defaultState or "RF"
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayerRef then
            AddAllowedUser(player.Name, defaultState)
        end
    end
    _G.RefreshVisuals()
end

_G.SetupUsers = function(userConfig)
    AllowedUsers = {}
    local myUsername = LocalPlayerRef.Name
    local myState = userConfig[myUsername] or "RF"
    
    for username, state in pairs(userConfig) do
        if username == myUsername then
            AddAllowedUser(username, state)
            print("Set your state to: " .. state)
        else
            AddAllowedUser(username, state)
            print("Added " .. username .. " with state: " .. state)
        end
    end
    
    _G.RefreshVisuals()
    print("Squad configured! Everyone paste the same code.")
end

_G.GetValidStates = function()
    print("=== Valid States ===")
    print("- RF (Red/Pink)")
    print("- Zoom (Blue)")
    print("- GodOfSpeed (White)")
    print("- NWA (Purple)")
    print("- KidFlash (Yellow)")
    print("- Regular (Orange/Red)")
    print("====================")
end

spawn(function()
    while task.wait(2) do
        for _, player in pairs(Players:GetPlayers()) do
            if IsAllowedUser(player) and player ~= LocalPlayerRef then
                if player.Character and not PlayerVisualData[player] then
                    InitializePlayerVisuals(player)
                    pcall(function()
                        local StarterGui = game:GetService("StarterGui")
                        StarterGui:SetCore("SendNotification", {
                            Title = "Shared Visuals",
                            Text = player.Name .. " visuals active!",
                            Duration = 3
                        })
                    end)
                elseif not player.Character and PlayerVisualData[player] then
                    CleanupPlayerVisuals(player)
                end
            end
        end
    end
end)

print("=== Shared Visuals System Loaded ===")
print("")
print("QUICK SETUP (All 3 players paste the SAME code):")
print("_G.SetupUsers({")
print("    ['Player1Name'] = 'Zoom',")
print("    ['Player2Name'] = 'RF',")
print("    ['Player3Name'] = 'GodOfSpeed'")
print("})")
print("")
print("OTHER COMMANDS:")
print("_G.AddUser('Username', 'State') - Add one person")
print("_G.SetUserState('Username', 'State') - Change color")
print("_G.GetAllowedUsers() - List everyone")
print("_G.RefreshVisuals() - Fix issues")
print("_G.GetValidStates() - See all states")
print("")
print("States: RF, Zoom, GodOfSpeed, NWA, KidFlash, Regular")
print("====================================")

AddAllowedUser(LocalPlayerRef.Name, "RF")
ParticleRef.Color = GetPartRefColorForState(GetUserState(LocalPlayerRef))
InitializePlayerVisuals(LocalPlayerRef)
