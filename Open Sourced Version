Running = false
ConnectionRef = nil

local PlayerVisualData = {}

local function InitializePlayerVisuals(player)
    if not CanSeeVisuals(player) then return end
    
    if PlayerVisualData[player] then
        return
    end
    
    PlayerVisualData[player] = {
        Character = player.Character,
        Trails = {},
        Effects = {},
        Connections = {}
    }
end

local function CleanupPlayerVisuals(player)
    local data = PlayerVisualData[player]
    if not data then return end
    
    for _, trail in pairs(data.Trails) do
        if trail and trail.Parent then
            trail:Destroy()
        end
    end
    
    for _, effect in pairs(data.Effects) do
        if effect and effect.Parent then
            effect:Destroy()
        end
    end
    
    for _, connection in pairs(data.Connections) do
        if connection then
            connection:Disconnect()
        end
    end
    
    PlayerVisualData[player] = nil
end

local function OnPlayerAdded(player)
    if not IsAllowedUser(player) and player ~= LocalPlayerRef then
        return
    end
    
    local function setupCharacter(character)
        task.wait(0.5)
        if character and character.Parent then
            CleanupPlayerVisuals(player)
            InitializePlayerVisuals(player)
        end
    end
    
    player.CharacterAdded:Connect(setupCharacter)
    
    if player.Character then
        setupCharacter(player.Character)
    end
end

local function OnPlayerRemoving(player)
    CleanupPlayerVisuals(player)
    local StarterGui = game:GetService("StarterGui")
    if IsAllowedUser(player) then
        pcall(function()
            StarterGui:SetCore("SendNotification", {
                Title = "Shared Visuals",
                Text = player.Name .. " disconnected",
                Duration = 3
            })
        end)
    end
end

Players.PlayerAdded:Connect(OnPlayerAdded)
Players.PlayerRemoving:Connect(OnPlayerRemoving)

for _, player in pairs(Players:GetPlayers()) do
    OnPlayerAdded(player)
end

LocalPlayerRef.CharacterAdded:Connect(function(character)
    CharacterRef = character
    HRP = character:WaitForChild("HumanoidRootPart")
    task.wait(1)
    CleanupPlayerVisuals(LocalPlayerRef)
    InitializePlayerVisuals(LocalPlayerRef)
end)

function EnableLightning()
    if Running then return end
    Running = true
    ParticleRef.Enabled = true
    ParticleRef:Emit(15)
    ConnectionRef = RunService.RenderStepped:Connect(function()
        if HRP and HRP.Parent then
            PartRef.CFrame = HRP.CFrame
        end
    end)
end

function DisableLightning()
    if not Running then return end
    Running = false
    ParticleRef.Enabled = false
    if ConnectionRef then
        ConnectionRef:Disconnect()
        ConnectionRef = nil
    end
end



