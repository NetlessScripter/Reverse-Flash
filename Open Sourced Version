            else

EnableLightningGlobal()

                StartEffect()
            end
        end)

        enableRunEvents()

        enableAll()
    else

StopEffect() 

DisableLightningGlobal()

DisableCoolStuff()

        if heartbeatConnection then
            heartbeatConnection:Disconnect()
            heartbeatConnection = nil
        end
        h.WalkSpeed = ws0

        if x then x:Destroy() end
        if a then a:Destroy() end

V88()

V90()

        StopEffect()
        if runningConnection then
            runningConnection:Disconnect()
            runningConnection = nil
        end

        disableRunEvents()
        disableAll()
    end

        elseif key == Enum.KeyCode.C then

    phs=not phs
    if phs then
        sn(true)

StartVibrate()

        EnableDarkAura()
    else

StopVibrate()

        sn(false)
        DisableDarkAura()
    end

        elseif key == Enum.KeyCode.B then

StopVibrate()

    DisableDarkAura()
    ang=not ang
    if ang then

V88()

        ocf=HRP.CFrame
        occ=cam.CameraType
        cam.CameraType=Enum.CameraType.Scriptable
        sn(true)
    else

V87()

        HRP.AssemblyLinearVelocity=Vector3.new()
        cam.CameraType=occ
        sn(false)
    end

elseif key == Enum.KeyCode.X then

    StopEffect()
    if heartbeatConnection then
        heartbeatConnection:Disconnect()
        heartbeatConnection = nil
    end
    if runningConnection then
        runningConnection:Disconnect()
        runningConnection = nil
    end
    disableRunEvents()
    disableAll()

    HRP.AssemblyLinearVelocity = Vector3.new()
    h.WalkSpeed = ws0
    ang = false
    phs = false
    spd = false

    if x then x:Destroy() end
    if a then a:Destroy() end

    DisableLightningGlobal()
    EnableAllCoolStuff()
    DisableCoolStuff()
    V88()
    V90()
    StopVibrate()
    StopVibrate()
    StopVibrate()
    DisableDarkAura()

    cam.CameraType = Enum.CameraType.Custom
    if cam.FieldOfView ~= 70 then
        local info = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut)
        local goal = { FieldOfView = 70 }
        ts:Create(cam, info, goal):Play()
    end

end
end
end)

local b = {
    -- R6 parts
    "Head",
    "Torso",
    "Left Arm",
    "Right Arm",
    "Left Leg",
    "Right Leg",
    -- R15 parts - Torso
    "UpperTorso",
    "LowerTorso",
    -- R15 parts - Arms
    "LeftUpperArm",
    "LeftLowerArm",
    "LeftHand",
    "RightUpperArm",
    "RightLowerArm",
    "RightHand",
    -- R15 parts - Legs
    "LeftUpperLeg",
    "LeftLowerLeg",
    "LeftFoot",
    "RightUpperLeg",
    "RightLowerLeg",
    "RightFoot"
}

local ig = workspace:WaitForChild(u):WaitForChild("HumanoidRootPart"):WaitForChild("Attachment")

function mk()
end

local godCS2 = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(255,255,255)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(255,255,255))
})

local nwaCS2 = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(210,160,255)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(140,0,255))
})

local zoomCS2 = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(160,220,255)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(0,120,255))
})

local kidCS = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(255,255,150)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(255,255,0))
})

local rfCS2 = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(255,84,84)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(255,0,0))
})

local regularCS2 = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(255,59,33)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(255,59,33))
})

spawn(function()
    while true do
        local cs = nil
        local spot = nil

        if _G.RF then
            cs = rfCS2
            spot = Color3.fromRGB(255,0,0)
        elseif _G.KidFlash then
            cs = kidCS
            spot = Color3.fromRGB(255,255,0)
        elseif _G.Zoom then
            cs = zoomCS2
            spot = Color3.fromRGB(0,120,255)
        elseif _G.GodOfSpeed and _G.NWA then
            if math.random(2) == 1 then
                cs = godCS2
                spot = Color3.fromRGB(255,220,0)
            else
                cs = nwaCS2
                spot = Color3.fromRGB(140,0,255)
            end
        elseif _G.GodOfSpeed then
            cs = godCS2
            spot = Color3.fromRGB(255,220,0)
        elseif _G.NWA then
            cs = nwaCS2
            spot = Color3.fromRGB(140,0,255)
        elseif _G.Regular then
            cs = regularCS2
            spot = Color3.fromRGB(255,59,33)
        else
            -- Default to Regular when no state is active
            cs = regularCS2
            spot = Color3.fromRGB(255,59,33)
        end

        if cs then
            mk()

            for _,n in pairs(b) do
                local pr = CharacterRef:FindFirstChild(n)
                if pr then
                    for _,t in pairs(pr:GetChildren()) do
                        if t:IsA("Trail") then
                            t.Color = cs
                        end
                    end
                end
            end

            for _,v in pairs(CharacterRef:GetDescendants()) do
                if v:IsA("ParticleEmitter") then
                    local cur = v
                    local g = false
                    while cur do
                        if cur == ig then
                            g = true
                            break
                        end
                        cur = cur.Parent
                    end

                    if v.Texture == "rbxassetid://9731596776" then
                        g = true
                    end
                    if v.Texture == "rbxassetid://10583327833" then
                        v.Color = cs
                        g = true
                    end

                    if not g then
                        v.Color = cs
                    end
                elseif v:IsA("SpotLight") then
                    v.Color = spot
                    v.Enabled = true
                end
            end
        end

        wait()
    end
end)





game:GetService("Players").PlayerAdded:Connect(function(Player)
	Player.CharacterAdded:Connect(function(Char)
		Char:WaitForChild("HumanoidRootPart").CustomPhysicalProperties = PhysicalProperties.new(1000, 0, 0)
	end)
end)
game:GetService("RunService").RenderStepped:Connect(function()
    local d = LocalPlayerRef.Character.Humanoid.MoveDirection
    if d.Magnitude > 0 then HRP.CFrame = CFrame.new(HRP.Position, HRP.Position + d) end
end)

local snap=18

RunService.RenderStepped:Connect(function(dt)
	if h.FloorMaterial==Enum.Material.Air then return end

	local md=h.MoveDirection
	if md.Magnitude==0 then return end

	md=Vector3.new(md.X,0,md.Z).Unit

	local v=HRP.AssemblyLinearVelocity
	local y=v.Y

	local sp=v.Magnitude
	if sp<1 then return end

	local target=md*sp
	HRP.AssemblyLinearVelocity=v:Lerp(Vector3.new(target.X,y,target.Z),math.clamp(dt*snap,0,1))
end)
