_G.RF = true

local AllowedUsers = {}

local ValidStates = {
    RF = true,
    Zoom = true,
    GodOfSpeed = true,
    NWA = true,
    KidFlash = true,
    Regular = true
}

local function AddAllowedUser(username, state)
    state = state or "RF"
    if not ValidStates[state] then
        warn("Invalid state: " .. state .. ". Using RF instead.")
        state = "RF"
    end
    AllowedUsers[username] = state
    print("Added " .. username .. " with state: " .. state)
end

local function RemoveAllowedUser(username)
    if AllowedUsers[username] then
        AllowedUsers[username] = nil
        print("Removed " .. username)
    end
end

local function IsAllowedUser(player)
    return AllowedUsers[player.Name] ~= nil
end

local function GetUserState(player)
    return AllowedUsers[player.Name] or "RF"
end

local function SetUserState(username, state)
    if not ValidStates[state] then
        warn("Invalid state: " .. state)
        return false
    end
    if AllowedUsers[username] then
        AllowedUsers[username] = state
        print("Updated " .. username .. " to state: " .. state)
        return true
    else
        warn(username .. " is not in allowed users. Add them first.")
        return false
    end
end

local function CanSeeVisuals(targetPlayer)
    local localPlayer = Players.LocalPlayer
    if localPlayer == targetPlayer then
        return true
    end
    return IsAllowedUser(localPlayer) and IsAllowedUser(targetPlayer)
end

local function GetColorForState(state)
    if state == "RF" then
        return OtherColors.RF
    elseif state == "Zoom" then
        return OtherColors.Zoom
    elseif state == "GodOfSpeed" then
        return OtherColors.GodOfSpeed
    elseif state == "NWA" then
        return OtherColors.NWA
    elseif state == "KidFlash" then
        return OtherColors.KidFlash
    else
        return OtherColors.Default
    end
end

local function GetPartRefColorForState(state)
    if state == "RF" then
        return PartRefColors.RF
    elseif state == "Zoom" then
        return PartRefColors.Zoom
    elseif state == "GodOfSpeed" then
        return PartRefColors.GodOfSpeed
    elseif state == "NWA" then
        return PartRefColors.NWA
    elseif state == "KidFlash" then
        return PartRefColors.KidFlash
    else
        return PartRefColors.Default
    end
end

