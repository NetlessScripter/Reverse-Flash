local ig = workspace:WaitForChild(u):WaitForChild("HumanoidRootPart"):WaitForChild("Attachment")

local godCS2 = OtherColors.GodOfSpeed2

local nwaCS2 = OtherColors.NWA2

local zoomCS2 = OtherColors.Zoom2

local kidCS = OtherColors.KidFlash

local rfCS2 = OtherColors.RF2

spawn(function()
    while true do
        local state = GetUserState(LocalPlayerRef)
        local cs = nil
        local spot = nil

        if state == "RF" then
            cs = rfCS2
            spot = Color3.fromRGB(255,0,0)
        elseif state == "KidFlash" then
            cs = kidCS
            spot = Color3.fromRGB(255,255,0)
        elseif state == "Zoom" then
            cs = zoomCS2
            spot = Color3.fromRGB(0,120,255)
        elseif state == "GodOfSpeed" then
            cs = godCS2
            spot = Color3.fromRGB(255,220,0)
        elseif state == "NWA" then
            cs = nwaCS2
            spot = Color3.fromRGB(140,0,255)
        else
            cs = OtherColors.Default
            spot = Color3.fromRGB(255,59,33)
        end

        if cs then
            for _,n in pairs(b) do
                local pr = CharacterRef:FindFirstChild(n)
                if pr then
                    for _,t in pairs(pr:GetChildren()) do
                        if t:IsA("Trail") then
                            t.Color = cs
                        end
                    end
                end
            end

            for _,v in pairs(CharacterRef:GetDescendants()) do
                if v:IsA("ParticleEmitter") then
                    local cur = v
                    local g = false
                    while cur do
                        if cur == ig then
                            g = true
                            break
                        end
                        cur = cur.Parent
                    end

                    if v.Texture == "rbxassetid://9731596776" then
                        g = true
                    end
                    if v.Texture == "rbxassetid://10583327833" then
                        v.Color = cs
                        g = true
                    end

                    if not g then
                        v.Color = cs
                    end
                elseif v:IsA("SpotLight") then
                    v.Color = spot
                    v.Enabled = true
                end
            end
        end

        wait()
    end
end)





game:GetService("Players").PlayerAdded:Connect(function(Player)
	Player.CharacterAdded:Connect(function(Char)
		Char:WaitForChild("HumanoidRootPart").CustomPhysicalProperties = PhysicalProperties.new(1000, 0, 0)
	end)
